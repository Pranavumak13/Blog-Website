<%- include("partials/header") -%>

    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input type="text" name="titleContent" class="form-control" >
        
            <label>Post</label>
            <textarea type="text" name="postContent" class="form-control"  rows="5" cols="30"></textarea>
        </div>
        <button type="submit" name="addContent" class="btn btn-primary">
            Publish
        </button>
    </form>

    <%- include("partials/footer") -%>